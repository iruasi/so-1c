# De momento, dado que todos los procesos se ejecutaran en la misma red local
# todas las direcciones IP de cada proceso seran el localhost

PUERTO_PROG=5000  # En este puerto, kernel recibe conexiones de Programas
PUERTO_CPU=5001   # En este puerto, kernel recibe conexiones de CPUs

IP_MEMORIA=127.0.0.1  # Dir IP del proceso memoria
PUERTO_MEMORIA=5002   # En este puerto, kernel se conecta a Memoria

IP_FS=127.0.0.1  # Dir IP del proceso filesystem
PUERTO_FS=5003   # En este puerto, kernel se conecta a Filesystem
QUANTUM=3
QUANTUM_SLEEP=500
ALGORITMO=RR

GRADO_MULTIPROG=1
SEM_IDS=[SEM1, SEM2, SEM3]
SEM_INIT=[0, 0, 5]
SHARED_VARS=[!Global, !UnaVar, !tiempo3]
STACK_SIZE=2
